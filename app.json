
{
  "expo": {
    "name": "coffee-shop",
    "slug": "coffee-shop",
    "version": "1.0.0",
    "deepLinking": true,
    "orientation": "portrait",
    "icon": "./assets/images/icon1.png",
    "scheme": "coffeeshop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon1.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/images/icon1.png",
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.coffeeshop",
      "googleServicesFile": "android/app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "447f1446-01ba-4192-b12e-a5c33adc3dd8"
      }
    },
    "owner": "nishthaa07"
  }
}
